<script id="partsVtx" type="x-shader/x-vertex">
  precision mediump float;
  uniform vec2 resolution;
  uniform vec2 typoPos;
  uniform float degree;
  uniform float time;
  //- ÊúÄÁµÇÂ∫ßÊ®ô
  attribute vec3 fixPosition;
  //- ÂàùÊúüÂ∫ßÊ®ô
  attribute vec3 initialPosition;
  //- Ë£úÂÆåÂ∫ßÊ®ô
  attribute vec3 completionPosition;
  attribute float delay;
  attribute float rotate;
  attribute vec2 texCoord;
  varying vec2 vTexCoord;
  varying float opacity;
  
  
  float easeQuartInOut(float t) {
    return t < 0.5 ? 8.0 * pow(t, 4.0) : -8.0 * pow(t - 1.0, 4.0) + 1.0;
  }
  float easeQuartOut(float t){
    return 1.0 - pow(1.0 - t, 4.0);
  }
  
  
  vec3 getRotate(vec3 p, float angle, vec3 axis){
      vec3 a = normalize(axis);
      float s = sin(angle);
      float c = cos(angle);
      float r = 1.0 - c;
      mat3 m = mat3(
          a.x * a.x * r + c,
          a.y * a.x * r + a.z * s,
          a.z * a.x * r - a.y * s,
          a.x * a.y * r - a.z * s,
          a.y * a.y * r + c,
          a.z * a.y * r + a.x * s,
          a.x * a.z * r + a.y * s,
          a.y * a.z * r - a.x * s,
          a.z * a.z * r + c
      );
      return m * p;
  }
  
  
  void main(){
    float scale = 5.0;
    float ratioX = (resolution.x / (2931.0));
    float ratioY = (resolution.y / (2095.0));
    float aspect = 2931.0 / 2095.0;
    float resAspect = resolution.x / resolution.y;
    float minRatio;
  
    if(aspect > resAspect){
      // Á∏¶ÂπÖ„Åå„Åß„Åã„ÅÑ
      minRatio = ratioX * 0.8;
    }else{
      // Ê®™ÂπÖ„Åå„Åß„Åã„ÅÑ
      minRatio = ratioY * 0.8;
    }
    float xx = cos(degree * 3.1415926535897932384626433832795 / 180.0) * 2931.0;
    float zz = sin(degree * 3.1415926535897932384626433832795 / 180.0);
  
    float duration = min(max((time - delay) , 0.0)  , 1.7) / 1.7;
    duration = easeQuartOut(duration);
    opacity = min(duration , 1.0);
    vec3 fixPositionR;
  
    if(degree == 0.0){
      fixPositionR = vec3((fixPosition.x)  * minRatio , (fixPosition.y + typoPos.y) * minRatio , fixPosition.z * minRatio);
  
    }else if(degree == 36.0){
      fixPositionR = getRotate(vec3((fixPosition.x + (2931.0 - xx) * 1.5) * minRatio , (fixPosition.y + typoPos.y) * minRatio , (fixPosition.z) * minRatio ) , degree * (3.1415926535897932384626433832795 / 180.0) , vec3(0.0 , 1.0 , 0.0));
  
    }else if(degree == -28.0){
      fixPositionR = getRotate(vec3((fixPosition.x - (2931.0 - xx) * 1.5) * minRatio, (fixPosition.y + typoPos.y) * minRatio , (fixPosition.z) * minRatio ) , degree * (3.1415926535897932384626433832795 / 180.0) , vec3(0.0 , 1.0 , 0.0));
    }else{
      fixPositionR = getRotate(vec3((fixPosition.x + (2931.0 - xx) * 1.5) * minRatio , (fixPosition.y + typoPos.y) * minRatio , (fixPosition.z) * minRatio ) , degree * (3.1415926535897932384626433832795 / 180.0) , vec3(0.0 , 1.0 , 0.0));
  
    }
  
  
    fixPositionR = vec3((fixPosition.x)  * minRatio , (fixPosition.y) * minRatio , fixPosition.z * minRatio);
  
    // fixPositionR = getRotate(vec3((fixPosition.x + (2931.0 - xx) * 1.5 + typoPos.x) * minRatio , (fixPosition.y + typoPos.y) * minRatio , (fixPosition.z) * minRatio ) , degree * (3.1415926535897932384626433832795 / 180.0) , vec3(0.0 , 1.0 , 0.0));
  
    // vec3 fixPositionR = vec3((fixPosition.x)  * minRatio , (fixPosition.y + typoPos.y) * minRatio , fixPosition.z * minRatio);
  
    vTexCoord = texCoord;
    vec3 hPositionDistance = (initialPosition - completionPosition);
    vec3 endPositionDistance = (fixPositionR - completionPosition);
    vec3 fDistance = (endPositionDistance * duration) + (hPositionDistance * (1.0 - duration));
    vec3 nPos = (fDistance * duration + completionPosition);
    vec3 rotateMtx = getRotate(nPos , (rotate * (1.0 - duration)) , vec3(1.0, 1.0, 1.0));
    vec4 mvPosition = modelViewMatrix * vec4(rotateMtx.x , rotateMtx.y * -1.0 , rotateMtx.z, 1.0);
    // vec4 mvPosition = modelViewMatrix * vec4(rotateMtx.x , rotateMtx.y * -1.0 , rotateMtx.z, 1.0);
    gl_Position = projectionMatrix * mvPosition;
  }
  
  
  
  
</script>
