<script id="logoSpVtx" type="x-shader/x-vertex">
  precision mediump float;
  uniform float time;
  uniform vec2 resolution;
  uniform vec2 typoPos;
  attribute vec2 texCoord;
  attribute vec3 fixPosition;
  attribute float delay;
  varying vec2 vTexCoord;
  varying float opacity;
  
  float easeQuartInOut(float t) {
    return t < 0.5 ? 8.0 * pow(t, 4.0) : -8.0 * pow(t - 1.0, 4.0) + 1.0;
  }
  
  void main(){
    float ratioX = (resolution.x / (2931.0));
    float ratioY = (resolution.y / (2095.0));
    float minRatio = min(ratioX , ratioY) * 0.8;
    float duration = min(max((time - delay) , 0.0)  , 3.0) / 3.0;
    duration = easeQuartInOut(duration);
    //- vec2 typoPos = vec2(0.0 , 0.0);
    opacity = min(duration , 1.0);
    vTexCoord = texCoord;
    vec3 fixPositionR = vec3(fixPosition.x * minRatio , fixPosition.y * minRatio , fixPosition.z) / 2.0;
    vec4 mvPosition = modelViewMatrix * vec4(fixPositionR.x , fixPositionR.y * -1.0 + typoPos.y , fixPositionR.z, 1.0);
    gl_Position = projectionMatrix * mvPosition;
  
  }
  
  
</script>
<script id="logoSpFrg" type="x-shader/x-fragment">
  uniform sampler2D tex;
  uniform float rand;
  varying vec2 vTexCoord;
  varying float opacity;
  
  void main(){
    vec4 textureC = texture2D(tex,vec2(vTexCoord.x  , vTexCoord.y));
    gl_FragColor = textureC * opacity;
  }
  
  
</script>
